<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.AyoubTarik.PatientArzt">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/hospital"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hospital"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clinic"
        tools:targetApi="31">



        <activity
            android:name="com.AyoubTarik.PatientArzt.patient_section.PatientActivity"
            android:exported="false" />
        <activity
            android:name="com.AyoubTarik.PatientArzt.doctor_section.DoctorActivity"
            android:exported="false" />

        <activity
            android:name="com.AyoubTarik.PatientArzt.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>